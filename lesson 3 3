var animals = ["cat", "dog", "bird", "raccoon", "bear", "rabbit", "coyote", "deer", "buffalo", "lizard", "turtle"];

console.log("The first animal in the list is a: ");
console.log(animals[0]);

console.log("The third animal in the list is a: ");
// print to the console the third animal using the list name and index
console.log(animals[3]);
console.log("The fifth animal in the list is a: ");
// print to the console the fifth animal using the list name and index
console.log(animals[5]);
console.log("The last animal in the list is a: ");
// print the last animal in the list to the console.
console.log(animals.length-1);
