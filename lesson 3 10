var animals = ["cat", "dog", 6, "bird", "raccoon", "bear", 11, "rabbit", "coyote", "deer", 13, "buffalo", "lizard", "turtle"];
removeItem(animals, 10);

// Add code to remove the other two numbers from the list
removeItem(animals,6);

removeItem(animals,2);

console.log(animals);



var months = ["January", "February", "March", "May", "June", "July", "August", "October", "December"];
insertItem(months, 3, "April");

// Add code to insert the months of "September" and "November" to their correct positions in the list.
insertItem(months,8,"September");
insertItem(months,10, "Novermber");
