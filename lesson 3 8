var food = ["carrots", "pizza", "lettuce", "strawberries"];
console.log("My food list: " + food);
console.log("Length of list: " + food.length);

appendItem(food, "peanuts");
console.log("My food list: " + food);
console.log("Length of list: " + food.length);

// use appendItem() to add an item to the food list
appendItem(food, "put a random food here");
console.log("My food list: " + food);
console.log("Length of list: " + food.length);

// use appendItem() to add an item to the food list
appendItem(food,"put a different food here");
console.log("My food list: " + food);
console.log("Length of list: " + food.length);
